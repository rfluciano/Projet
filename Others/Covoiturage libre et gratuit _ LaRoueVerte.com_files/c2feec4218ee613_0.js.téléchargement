};LaRoueVerte.Map.Geometry =Class.create();LaRoueVerte.Map.Geometry.prototype ={initialize :function(wktStringOrNativeObject,lrvMap,drawOptions,forceToType) {this.gPolygon =null;this.isMultiPolygon =false;this.gPolyline =null;this.gGeometry =null;if (isDefined(lrvMap)) {this.gMap =lrvMap.map;}
